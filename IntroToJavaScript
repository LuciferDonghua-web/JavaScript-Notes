    // + - / * :-

function sum(num1, num2){
  return num1 + num2;
}

function subtract(num1, num2){
  return num1 - num2;
}

function multiply(num1, num2){
  return num1 * num2;
}

function divide(num1, num2){
  return num1 / num2;
}


function calculate(num1, num2, func) {
    func(num1, num2)

}


console.log(calculate(10, 2, subtract)); 

        

    





